{
    "spellright.language": ["English (American)", "German_de_DE"],
    "spellright.documentTypes": ["markdown", "latex", "plaintext", "javascript", "php"],
    "files.exclude": {
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.log": true,
        "**/*.aux": true,
        "**/*.bcf": true,
        "**/*.out": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.synctex(busy)": true,
        "**/*.toc": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*-blx.bib": true,
        "**/_minted*": true,
        "**/__pycache__": true
    },
    "python.analysis.extraPaths": ["./slack", "./managing_imagenet_data", "./models/helpers", "./models"]
}
